@model Bugtracker.ViewModels.DeleteUsersViewModel

@{
    Layout = null;
}

    <form class="text-center border border-light p-5" asp-controller="ManageUsers" asp-action="DeleteUsers" asp-route-projectId="@Model.projectId" asp-route-userId="@Model.UserId" onsubmit="return jQueryAjaxPost(this);" autocomplete="off">

        <p class="h4 mb-4">Delete User</p>

        

        <div style="height:30px"></div>


        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <button class="btn btn-primary" type="submit">Confirm</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
    </form>